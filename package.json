{
  "name": "webpack-playground",
  "version": "1.0.0",
  "description": "webpack playground",
  "main": "index.js",
  "private": true,
  "scripts": {
    "use_env": "webpack --env MODE=prod --env USE_ANALYZER",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "start": "webpack serve --config webpack.dev.js --open",
    "stats": "webpack --config webpack.dev.js --profile --json > stats.json",
    "analyze": "npm run stats & webpack-bundle-analyzer stats.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "babel-loader": "^10.0.0",
    "browserslist": "^4.25.0",
    "css-loader": "^7.1.2",
    "csv-loader": "^3.0.5",
    "html-webpack-plugin": "^5.6.3",
    "json5": "^2.2.3",
    "style-loader": "^4.0.0",
    "toml": "^3.0.0",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^6.0.1",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "core-js": "^3.42.0",
    "lodash": "^4.17.21",
    "regenerator-runtime": "^0.14.1"
  }
}
